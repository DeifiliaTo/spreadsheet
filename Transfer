package spreadsheet;

public class Transfer {
	
	int[] date = new int[3];
	int amount;
	String payer;
	String payee;
	String purpose;
	
	
	public Transfer(int[] date, int amount, String payer, String payee, String purpose) {
		this.date = date;
		this.amount = amount;
		this.payer = payer;
		this.payee = payee;
		this.purpose = purpose;
	}
}
