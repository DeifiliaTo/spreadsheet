package spreadsheet;

public class Transfer {
	
	int[] date = new int[3];
	double amount;
	User payer;
	User payee;
	String purpose;
	
	
	public Transfer(int[] date, double amount, User payer, User payee, String purpose) {
		this.date = date;
		this.amount = amount;
		this.payer = payer;
		this.payee = payee;
		this.purpose = purpose;
	}
}
