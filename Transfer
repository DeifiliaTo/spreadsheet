package spreadsheet;

public class Transfer {
	
	int[] date = new int[3];
	int amount;
	String payer;
	String recipient;
	String purpose;
	
	
	public Transfer(int[] date, int amount, String payer, String recipient, String purpose) {
		this.date = date;
		this.amount = amount;
		this.payee = payer;
		this.recipient = recipient;
		this.purpose = purpose;
	}
}
